<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="MySpace Automation Suite" thread-count="15" parallel="none">
  	
 	<listeners>
		<listener class-name="com.innominds.team.listeners.TestReportListner" />
	</listeners> 
  
	<test name="MySpace web Automation Tests_firefox" enabled="true">
		<parameter name="browser" value="firefox" />
		<parameter name="os" value="WIN10" />
		<classes>
			<class name="com.web.tests.webtestcases.ifusion.MySpaceTest" />
		</classes>
	</test>
 
	 <test name="MySpace web Automation Tests_chrome" enabled="true">
		<parameter name="browser" value="chrome" />
		<parameter name="os" value="WIN10" />
	    <classes>
			<class name="com.web.tests.webtestcases.ifusion.MySpaceTest" />
		</classes>
	</test> 

</suite>